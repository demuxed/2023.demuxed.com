{
	"useTabs": true,
	"printWidth": 120,
	"singleQuote": true,
	"tabWidth": 2,
	"semi": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"importOrder": [
		"^react(.*)$",
		"^next(.*)$",
		"<THIRD_PARTY_MODULES>",
		"^~/(.*)$",
		"^@/(.*)$",
		"^app/_(.*)$",
		"^app/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
